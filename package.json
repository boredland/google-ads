{
  "name": "@htdangkhoa/google-ads",
  "description": "Google Ads API client for Node.js",
  "version": "0.0.0",
  "homepage": "https://github.com/htdangkhoa/google-ads",
  "bugs": {
    "url": "https://github.com/htdangkhoa/google-ads/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/htdangkhoa/google-ads.git"
  },
  "author": "htdangkhoa <huynhtran.dangkhoa@gmail.com> (https://github.com/htdangkhoa)",
  "contributors": [
    "huynhtran.dangkhoa@gmail.com"
  ],
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "default": "./dist/esm/index.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "main": "./dist/cjs/lib/index.js",
  "module": "./dist/esm/lib/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "jest --runInBand --no-cache",
    "test:cov": "npm run test && coveralls < coverage/lcov.info",
    "prebuild": "rimraf dist",
    "build": "npm run build:esm && npm run build:cjs",
    "build:esm": "node scripts/build/esm.mjs",
    "build:cjs": "node scripts/build/cjs.mjs",
    "build:types": "node scripts/build/types.mjs",
    "generate": "./scripts/generate.sh"
  },
  "dependencies": {
    "@bufbuild/protobuf": "^2.1.0",
    "@grpc/grpc-js": "^1.11.3",
    "deepmerge": "^4.3.1",
    "import-sync": "^2.2.2",
    "log4js": "^6.9.1"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.30",
    "coveralls-next": "^4.2.0",
    "glob": "^10.3.10",
    "googleapis": "^134.0.0",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "rimraf": "^5.0.5",
    "string-width": "^4.2.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "ts-proto": "^2.2.0",
    "typescript": "5.4.3"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}
